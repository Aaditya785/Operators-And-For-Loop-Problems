import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here

        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int rev = 0, rem = 0;
        for(int num=N;num>=1;num = num/10)

        {
            rem = num % 10;
            rev = rev * 10 + rem;
        }
        System.out.print(rev);
        
	}
}
